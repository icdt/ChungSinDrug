
@{
    var htmlfieldprefix = ViewData.TemplateInfo.HtmlFieldPrefix;
    var formattedmodelvalue = ViewData.TemplateInfo.FormattedModelValue;
    var group = Html.GetUIHintParametersValue<string>("DropDownListName");
    string name = DropDownListHelper.GetName(group, formattedmodelvalue.ToString());

}

@if (!string.IsNullOrEmpty(formattedmodelvalue.ToString()))
{
    @Html.Display(name)
}
